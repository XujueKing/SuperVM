impl ZkProofScheduler {
    /// è°ƒåº¦ ZK è¯æ˜ä»»åŠ¡
    pub async fn schedule_proof(&self, proof_task: ZkProofTask) -> Result<Proof> {
        // 1. ä¼˜å…ˆå°è¯• GPU èŠ‚ç‚¹
        if let Some(gpu_node) = self.find_available_gpu_node() {
            match self.submit_to_gpu(gpu_node, proof_task.clone()).await {
