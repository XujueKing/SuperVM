// src/node-core/src/zk_scheduler.rs

pub struct ZkProofScheduler {
    gpu_nodes: Vec<NodeId>,  // æœ‰ GPU çš„ L1 èŠ‚ç‚¹
    cpu_fallback: Vec<NodeId>,
